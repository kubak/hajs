{
   "env": {
      "node": true
   },
   "parserOptions": {
       "ecmaVersion": 9,
       "sourceType": "module"
   },
   "extends" : "eslint:recommended",
   "rules": {
       "semi": 2,
       "curly": 2,
       "eqeqeq": 2,
       "no-eq-null": 2,
       "no-console": 0,
       "no-unused-vars": [2, { "args": "none", "ignoreRestSiblings": true }],
       "prefer-object-spread": 1,
       "prefer-spread": 1,
       "rest-spread-spacing": [1, "never"],
       "dot-notation": 1
   }
}